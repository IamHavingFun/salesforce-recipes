<aura:component controller="DynamicFlowCtrl" implements="flexipage:availableForRecordHome,force:hasRecordId">

   <!-- Lax -->
   <c:lax context="{!this}" />

   <!-- Events -->
   <aura:handler name="init" value="{!this}" action="{!c.init}"/>

   <!-- Markup -->
   <c:resultToast aura:id="toast" />
   <lightning:flow aura:id="wrapper"/>
   
</aura:component>